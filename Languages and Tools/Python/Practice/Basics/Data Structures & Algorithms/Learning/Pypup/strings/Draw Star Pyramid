# Type of "n" = int
def stars_vii(n):
    for i in range(1, n + 1):
        print(' ' * (n - i) + '*' * (2 * i - 1))

stars_vii(5)

def stars_vii2(n):
   for i in range(1, n + 1):
       for _ in range(n - i):
           print(" ", end="")
       for _ in range(* i - 1):
           print("*", end="")
       print()

stars_vii2(5)